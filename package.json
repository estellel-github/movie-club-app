{
  "name": "movie-club-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "docker compose -f docker-compose.dev.yml --env-file .env.dev up --build",
    "prod": "docker compose -f docker-compose.prod.yml --env-file .env.prod up --build",
    "start": "node dist/index.js",
    "stop:dev": "docker compose -f docker-compose.dev.yml --env-file .env.dev down --volumes",
    "stop:prod": "docker compose -f docker-compose.dev.yml --env-file .env.prod down --volumes",
    "start:local": "NODE_ENV=local tsx src/index.ts",
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "build": "tsc",
    "migrate": "ts-node src/migrations/run.ts",
    "inspect": "docker inspect movieclub_dev_app | grep IPAddress",
    "seed": "ts-node ./seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "argon2": "^0.41.1",
    "bullmq": "^5.34.10",
    "dotenv": "^16.4.7",
    "eslint-define-config": "^2.1.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.14",
    "typeorm": "^0.3.20",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "madge": "^8.0.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
